{
    "workbench.colorTheme": "Dracula",
    "workbench.iconTheme": "material-icon-theme",
    "editor.tabSize": 2,
    "actionButtons": {
        "loadNpmCommands": false,
        "commands": [
            {
                "name": "Install",
                "cwd": "${workspaceFolder}",
                "command": "yarn install",
                "color": "red"
            },
            {
                "name": "B FRONT",
                "cwd": "${workspaceFolder}/front",
                "command": "yarn build",
                "color": "yellow"
            },
            {
                "name": "B API",
                "cwd": "${workspaceFolder}/backend",
                "command": "yarn build",
                "color": "yellow"
            },
            {
                "name": "FRONT",
                "cwd": "${workspaceFolder}/front",
                "command": "yarn start",
                "color": "cyan"
            },
            {
                "name": "API",
                "cwd": "${workspaceFolder}/backend",
                "command": "yarn start:debug",
                "color": "cyan"
            },
            {
                "name": "DB",
                "cwd": "${workspaceFolder}/database",
                "command": "docker-compose up",
                "color": "cyan"
            }
        ]
    },
    "cSpell.words": [
        "browserslist",
        "głównej",
        "Mercurius",
        "Powrót",
        "strony"
    ],
}